if(travel_direction = "right")
{
	x += bullet_speed;
	image_angle = 270;
}
else if(travel_direction = "up")
{
	y -= bullet_speed
	image_angle = 0
}
else if(travel_direction = "down")
{
	y += bullet_speed
	image_angle = 180
}
else if(travel_direction = "left")
{
	x -= bullet_speed
	image_angle = 90
	
}
